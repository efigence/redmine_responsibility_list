<thead>
  <tr>
    <th><%= t(:project) %></th>
    <th><%= t(:code_name) %></th>
    <th><%= t(:url) %></th>
    <th><%= Setting.plugin_redmine_responsibility_list[:roles][:role1][:title] %></th>
    <th><%= Setting.plugin_redmine_responsibility_list[:roles][:role2][:title] %></th>
    <th><%= Setting.plugin_redmine_responsibility_list[:roles][:role3][:title] %></th>
    <th><%= Setting.plugin_redmine_responsibility_list[:roles][:role4][:title] %></th>
    <th><%= Setting.plugin_redmine_responsibility_list[:roles][:role5][:title] %></th>
    <% if @list_handler.custom_fields %>
      <% @list_handler.custom_fields.each do |custom_field| %>
        <th><%= custom_field.name %></th>
      <% end %>
    <% end %>
    <th></th>
  </tr>
</thead>
